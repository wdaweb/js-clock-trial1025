  "license": "MIT",
  "devDependencies": {
    "@antfu/ni": "^0.18.8",
    "@babel/core": "^7.22.19",
    "@types/node": "^18.17.15",
    "browser-sync": "^2.29.3",
    "browser-sync-webpack-plugin": "^2.3.0",
    "bumpp": "^8.2.1",
    "prettier": "2.8.3",
    "ts-loader": "^9.4.4",
    "typescript": "^4.9.5",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "tsup": "^6.7.0"
